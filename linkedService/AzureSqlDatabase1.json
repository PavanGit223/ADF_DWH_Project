{
	"name": "AzureSqlDatabase1",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"Server_Name_dwh": {
				"type": "string"
			},
			"db_name_dwh": {
				"type": "string"
			},
			"key_secret_dwh": {
				"type": "string"
			},
			"db_username": {
				"type": "string"
			},
			"key_version": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"server": "@{concat(linkedService().Server_Name_dwh,'.database.windows.net')}",
			"database": "@{linkedService().db_name_dwh}",
			"encrypt": "mandatory",
			"trustServerCertificate": false,
			"authenticationType": "SQL",
			"userName": "@{linkedService().db_username}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ADF_key_valut",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().key_secret_dwh",
					"type": "Expression"
				},
				"secretVersion": {
					"value": "@linkedService().key_version",
					"type": "Expression"
				}
			}
		}
	}
}